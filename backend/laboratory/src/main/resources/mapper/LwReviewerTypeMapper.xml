<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zj.laboratory.mapper.LwReviewerTypeMapper">

    <resultMap id="BastResultMap" type="com.zj.laboratory.pojo.LwReviewerType">
        <id column="id" property="id"/>
        <result column="type_id" property="typeId"/>
        <result column="name" property="name"/>
        <result column="order_num" property="orderNum"/>
    </resultMap>
    <select id="getList" resultMap="BastResultMap">
        select id, type_id, name, order_num
        from lw_reviewer_type
    </select>

</mapper>